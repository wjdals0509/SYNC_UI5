<mvc:View controllerName="sync.e21.odatamodel.controller.Main"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:t="sap.ui.table">
    <Page id="page" title="{i18n>title}">
        <content>
            <t:Table visibleRowCount="10"
                rows="{/CarrierSet}">
                <t:columns>
                    <t:Column >
                        <Label text="{i18n>labelCarrid}" /> 
                        <t:template>
                            <ObjectIdentifier title="{Carrid}"></ObjectIdentifier>
                        </t:template>
                    </t:Column>
                    <t:Column >
                        <Label text="{i18n>labelCarrname}" /> 
                        <t:template>
                            <ObjectAttribute text="{Carrname}"></ObjectAttribute>
                        </t:template>
                    </t:Column>
                    <t:Column >
                        <Label text="{i18n>labelCurrcode}" /> 
                        <t:template>
                            <ObjectAttribute text="{Currcode}"></ObjectAttribute>
                        </t:template>
                    </t:Column>
                    <t:Column >
                        <Label text="{i18n>labelUrl}" /> 
                        <t:template>
                            <Link text="웹페이지 이동" href="{Url}"></Link>
                        </t:template>    
                    </t:Column>
                </t:columns>
            </t:Table>
        </content>
    </Page>
</mvc:View>
